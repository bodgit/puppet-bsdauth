{
  "name": "bodgit-bsdauth",
  "version": "1.0.2",
  "author": "bodgit",
  "summary": "Puppet Module for managing BSD authentication",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-bsdauth",
  "project_page": "https://github.com/bodgit/puppet-bsdauth",
  "issues_url": "https://github.com/bodgit/puppet-bsdauth/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.2",
        "6.3"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.4.0 <6.0.0"
    }
  ],
  "tags": [
    "bsd",
    "auth",
    "ldap",
    "login.conf"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.13.0 <5.0.0"
    },
    {
      "name": "bodgit/bodgitlib",
      "version_requirement": ">=1.6.0 <3.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=2.1.0 <5.0.0"
    },
    {
      "name": "thrnio/ip",
      "version_requirement": ">=1.0.0 <2.0.0"
    }
  ]
}
